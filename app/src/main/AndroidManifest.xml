<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yildizozan.melikserif">

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.yildizozan.melikserif.LoginActivity" >

        </activity>

        <activity android:name="com.yildizozan.melikserif.PasswordVerifyActivity" ></activity>

        <activity android:name="com.yildizozan.melikserif.RegisterActivity" ></activity>

        <activity android:name="com.yildizozan.melikserif.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.yildizozan.melikserif.NewsShowActivity" ></activity>

    </application>

</manifest>
